		"emojify",
		"hyperborea",
		"jsonpath",
		"shlib",
		// peeple names:
		"belyakov",
		"mclarkson",
		"mrowa", // mrowa44
		// system packages:
		"acodec",
		"alsa",
		"apcupsd",
		"asound",
		"cmos",
		"emms",
		"exwm",
		"ffmpeg",
		"fusermount",
		"gtkrc",
		"libmp", // libmp3lame
		"marcfs",
		"nixos",
		"nixpkgs",
		"nvram",
		"passff",
		"rclone",
		"systemctl",
