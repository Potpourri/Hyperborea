{
	"version": "0.1",
	"language": "en",
	"overrides": [
		{
			"filename": "**/tools/*",
			"languageId": "shellscript"
		}
	],
	"allowCompoundWords": true,
	"words": [
		// bash keywords\builtins, coreutils\findutils and other GNU tools:
